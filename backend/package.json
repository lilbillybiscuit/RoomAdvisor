{
  "name": "roomadvisor_backend",
  "version": "1.0.0",
  "description": "backend for the roomadvisor website",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "prod": "pm2 startOrReload ecosystem.config.js --env production",
